{
  "name": "boilertemplate",
  "displayName": "displayName",
  "version": "0.0.1",
  "description": "description",
  "repository": {
    "type": "git",
    "url": "https://github.com/Example/Example"
  },
  "license": "MIT",
  "publisher": "CatBraaain",
  "main": "dist/extension.js",
  "scripts": {
    "watch": "conc npm:watch:*",
    "watch:1:typecheck": "tsc -w -noEmit --preserveWatchOutput",
    "watch:2:compile": "vite build -w",
    "watch:3:config": "nodemon -w vite.config.mts -x \"vite build\""
  },
  "contributes": {
    "commands": [
      {
        "command": "boilertemplate.helloWorld",
        "title": "boilertemplate: Hello World"
      }
    ]
  },
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@eslint/compat": "^1.2.2",
    "@eslint/js": "^9.11.1",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.10",
    "@types/vscode": "^1.93.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.11.1",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-autofix": "^2.1.0",
    "eslint-plugin-package-json": "^0.15.5",
    "eslint-plugin-unicorn": "^56.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "rollup-plugin-node-externals": "^8.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^6.0.7"
  },
  "engines": {
    "vscode": "^1.19.0"
  }
}
